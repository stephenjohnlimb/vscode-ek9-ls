    "prefix": "_op",
    "body": [  
      "operator ${1|++,--|}",      
      "\t$0",      
      ""
    ],
    "description": "Operator mutation"
  },
  "Operator and return" : {
    "prefix": "_opr",
    "body": [  
      "operator ${1|$,?,!,#<,#>,#?,#^,length,empty|}",
      "\t<-${2:rtn} as $3: $4",
      "\t$0",      
      ""
    ],
    "description": "Operator and return"
  },
